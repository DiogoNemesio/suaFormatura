<?php

namespace Entidades;

use Doctrine\ORM\Mapping as ORM;

/**
 * ZgfmtRifaVendaSequencial
 *
 * @ORM\Table(name="ZGFMT_RIFA_VENDA_SEQUENCIAL")
 * @ORM\Entity
 */
class ZgfmtRifaVendaSequencial
{
    /**
     * @var integer
     *
     * @ORM\Column(name="CODIGO", type="integer", nullable=false)
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="IDENTITY")
     */
    private $codigo;


    /**
     * Get codigo
     *
     * @return integer 
     */
    public function getCodigo()
    {
        return $this->codigo;
    }
}
